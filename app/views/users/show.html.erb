<%binding.pry%>
<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1>Hi there, <%= @user.name %></h1>
<p>Your Mood: <%= @user.mood %></p>
<p>Your Tickets: <%= @user.tickets %></p>
<p>Your height: <%= @user.height %></p>

<p><%= link_to "See attractions", attractions_path %></p>

<%= form_tag ({controller: 'sessions', action: 'destroy', method: 'post'}) do %>
  <%= link_to "Log Out", '/logout'%>
<% end %>
